{
  "name": "mdvideo",
  "version": "2.0.6",
  "description": "一款简单的视频编辑器，支持Markdown转视频输出",
  "main": "./out/main/index.js",
  "author": "lq02",
  "homepage": "https://mdvideo.wvovw.com",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "rebuild": "./node_modules/.bin/electron-rebuild.cmd",
    "start": "chcp 65001 && electron-vite dev --watch",
    "start:renderer": "chcp 65001 && electron-vite dev --watch --rendererOnly=true",
    "preview": "electron-vite preview",
    "build": "npm run typecheck && electron-vite build",
    "publish": "node ./publish  && surge ./dist/publish mdvideo.surge.sh",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "build:win": "electron-vite build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@codemirror/lang-markdown": "^6.1.1",
    "@codemirror/language-data": "^6.3.0",
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@gausszhou/vue3-drag-resize-rotate": "^3.0.0-beta.4",
    "@lhlyu/vue-virtual-waterfall": "^1.0.2",
    "@seald-io/nedb": "^4.0.1",
    "@trodi/electron-splashscreen": "^1.0.2",
    "@types/markdown-it": "^12.2.3",
    "@vavt/util": "^1.1.1",
    "@vue/shared": "^3.3.4",
    "@vueuse/core": "^9.13.0",
    "ark.db": "^2.7.1",
    "copy-to-clipboard": "^3.3.3",
    "cos-nodejs-sdk-v5": "^2.12.4",
    "crypto-js": "^4.1.1",
    "echarts": "^5.1.2",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "ffcreator": "^7.2.2",
    "fluent-ffmpeg": "2.1.2",
    "formidable": "^2.0.1",
    "ip": "^1.1.8",
    "medium-zoom": "^1.0.8",
